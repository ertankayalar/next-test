{
    "version": "0.1.0",
    "configurations": [

        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/",
            "webRoot": "${workspaceFolder}/public",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
              ]
        },
        {
            "name": "Launch category",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/public/index.html"
        },
    
    ],
    
}